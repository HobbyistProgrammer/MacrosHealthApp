<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500" prefWidth="340" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demomacros.AddFoodController">
   <ListView fx:id="listSearched" layoutX="14.0" layoutY="120.0" prefHeight="370.0" prefWidth="313.0" />
   <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBackButtonClicked" style="-fx-background-radius: 50px;" text="&lt;-">
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
   </Button>
   <TextField fx:id="txtSearch" layoutX="96.0" layoutY="53.0" />
   <Label layoutX="80.0" layoutY="20.0" prefHeight="27.0" prefWidth="182.0" text="Search For Products">
      <font>
         <Font name="System Bold" size="18.0" />
      </font>
   </Label>
   <Button layoutX="140.0" layoutY="84.0" mnemonicParsing="false" onAction="#findProductByName" prefHeight="25.0" prefWidth="61.0" text="Button" />
   <Label layoutX="253.0" layoutY="58.0" text="Per/Serving">
      <font>
         <Font size="11.0" />
      </font>
   </Label>
</AnchorPane>
